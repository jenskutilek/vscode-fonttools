{
    "name": "fonttools",
    "displayName": "FontTools",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ttx",
            "aliases": ["TTX", "ttx"],
            "extensions": [".ttx"],
            "configuration": "./language-configuration.json"
        },{
            "id": "ttxasm",
            "aliases": ["TTX Assembly", "ttxasm"],
            "extensions": [".ttxasm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ttx",
            "scopeName": "text.xml.ttx",
            "path": "./syntaxes/ttx.tmLanguage.json",
            "embeddedLanguages": {
                "meta.embedded.block.assembly": "source.assembly"
            }
        },{
            "language": "ttxasm",
            "scopeName": "source.assembly",
            "path": "./syntaxes/ttxasm.tmLanguage.json"
        }]
    }
}